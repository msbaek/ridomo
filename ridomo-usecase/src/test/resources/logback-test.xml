<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE configuration>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>
				<![CDATA[[%date{yyyy-MM-dd HH:mm:ss}] %-5level %logger.%method\(%file:%line\) - %message%n]]></pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<appender-ref ref="STDOUT"/>
	</root>
</configuration>
